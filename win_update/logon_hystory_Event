 Get-WinEvent -path 'C:\Windows\System32\winevt\Logs\Security.evtx' | Where {$_.Id -match '4624|4634'} |Where {$_.TimeCreated -gt (Get-Date).AddDays(-1)} | select  @{Name='TimeGenerated';Expression={$_.'TimeCreated'}}, @{Name='EventID';Expression={$_.'Id'}},@{Name='TaskCategory';Expression={$_.'TaskDisplayName'}},@{Name='SubjectUserSid';Expression={([xml]$_.ToXml()).Event.EventData.Data[0].'#text'}},@{Name='AccountName';Expression={([xml]$_.ToXml()).Event.EventData.Data[1].'#text'}}, @{Name='LogonProcessName';Expression={([xml]$_.ToXml()).Event.EventData.Data[11].'#text'}}-First 9  | Format-table -AutoSize

